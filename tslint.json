{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "lib/**/*.d.ts"
        ]
    },
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "max-line-length": false
    }
}
