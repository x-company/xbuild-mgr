{
    "restartable": "rs",
    "ignore": [
        "**/*.tests.ts",
        ".git",
        "node_modules"
    ],
    "watch": [
        "src",
        "tests/integration/xinit"
    ],
    "verbose": false,
    "execMap": {
        "ts": "node --require ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}
